SELECT 
ob.organization_id,
ob.status,
ob.updated_at,
os.trailing_seven,
max(acc.status) as "account_status"
FROM organizational_bills ob
LEFT JOIN organization_statistics os on os.organization_id = ob.organization_id
LEFT JOIN account_statuses acc on acc.organization_id = ob.organization_id
WHERE ob.organization_id IN (6,611,679,936,1219,1253,1837,2130,2194,2205,2242,2501,2683,2691,2869,2887,3062,3093,3161,3356,3637,3817,3878,4321,4770,5052,5234,5239,5439,5637,5699,5758,5848,5890,6299,6386,6401,6421,6815,6876,6919,6974,7107,7137,7504,7891,8197,8229,8484,8502,8868,8971,9563,10206,10523,10590,10676,10728,11077,11690,11775,11850,12146,12244,12384,12591,12805,12816,12974,13495,13544,13721,13809,14483,14484,14499,15494,15537,15658,15772,15860,15974,16040,16352,16390,16476,16514,16566,16713,16929,17023,17154,17158,17186,17715,17837,18166,18350,18646,18940,19150,19183,19276,19361,19449,19662,20133,20337,20407,20644,21012,21246,21282,21385,21408,21553,21595,21818,21994,22428,22764,23311,23938,24265,24565,24724,25165,25256,25443,25515,25532,25645,25679,25725,26176,26181,26591,26796,27037,27393,27429,27590,27704,27723,27811,28063,28145,28336,28482,28685,28805,28957,29042,29098,29125,29198,29200,29202,29231,29399,29413,29426,29481,29598,29609,29674,29807,29833,29837,29886,29891,29904,29908,30083,30165,30250,30279,30357,30379,30395,30422,30496,30621,30660,30733,30770,30990,31127,31157,31371,31454,31495,31787,32047,32177,32327,33372,33600,33663,33748,34061,34528,34545,35020,35126,35142,35336,35338,35351,35625,35729,35776,35813,35988,36292,36300,36449,36503,36729,36977,37361,37417,37518,37534,37572,37722,37795,38025,38318,38328,38456,38710,38734,38820,38874,39475,39611,39643,39679,39778,39786,39788,39821,39830,40169,40211,40396,40482,40665,40723,40733,40739,40800,40855,40938,41230,41327,41372,41385,41390,41672,41752,41987,42018,42053,42161,42225,42238,42280,42316,42333,42393,42394,42413,42429,42450,42516,42590,42595,42761,42808,42816,42819,42872,42925,43070,43121,43152,43276,43314,43353,43402,43422,43513,43564,43614,43700,43783,43789,43860,44095,44129,44184,44219,44246,44305,44339,44349,44352,44442,44487,44514,44582,44708,44768,44877,45080,45122,45291,45329,45340,45346,45362,45403,45504,45573,45627,45720,45916,46066,46189,46210,46292,46538,46643,46663,46745,46920,47